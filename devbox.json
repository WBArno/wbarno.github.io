{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": [
    "git@latest",
    "go@latest",
    "hugo@latest",
    "nodejs@latest"
  ],
  "shell": {
    "init_hook": [
      "sh ./.devcontainer/devbox_init.sh > /dev/null"
    ],
    "scripts": {
      "launch_server": [
        "hugo server -w"
      ]
    }
  }
}
